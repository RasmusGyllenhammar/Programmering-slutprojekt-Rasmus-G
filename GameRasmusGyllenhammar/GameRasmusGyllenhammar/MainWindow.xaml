<Window x:Class="GameRasmusGyllenhammar.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameRasmusGyllenhammar"
        mc:Ignorable="d"
        Title="MainWindow" Height="90" Width="400" MaxHeight="90" MaxWidth="400">
    <StackPanel>
        <TextBlock Background="white">Welcome! Choose to start game or go in to settings!</TextBlock>
        <Button Background="White" Name="startButton" Click="button_start">Start Game</Button>
        <Button Background="White" Name="settings" Click="button_close">Close</Button>
    </StackPanel>
</Window>
