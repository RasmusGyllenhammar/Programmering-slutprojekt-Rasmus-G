<Window x:Class="GameRasmusGyllenhammar.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameRasmusGyllenhammar"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas Name="myCanvas" Background="LightBlue" Focusable="True" KeyDown="KeyIsDown" KeyUp="KeyIsUp">
        <Label Content="red score: "/>
        <Label Content="green score: "/>
        <Rectangle Name="player" Height="130" Width="27" Fill="red" Canvas.Left="40" Canvas.Top="250" Tag="paddle"/>
      
        <Rectangle Name="playerTwo" Height="130" Width="27" Fill="Green" Canvas.Left="720" Canvas.Top="250" Tag="paddle"/>
        <Rectangle Name="ball" Height="25" Width="25" Fill="white" Canvas.Left="200" Canvas.Top="300" />
            
    </Canvas>
</Window>
